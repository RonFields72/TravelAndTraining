, a.Requested_By_Full_Name
, a.Requested_DateTime
, a.[Status]
, a.Status_Description
, a.Total_Cost 
, c.Final_Reviewed_By


FROM v_View_Requests a 
Left Outer Join dbo.Approval_Audit b on a.Id = b.Request_Id
Left Outer Join dbo.Travel_Training_Request c on a.Id = c.Id
WHERE a.[Status] = 'APR'
	 AND  c.Final_Reviewed_By = @Reviewed_By
	 AND a.[Status] &lt;&gt; 'PEN'
	 AND a.[Status] &lt;&gt; 'RTE'
	 AND a.[Status] &lt;&gt; 'NEW'
	 AND a.[Status] &lt;&gt; 'DNY'
   
GROUP BY a.Event_Name
, a.Event_Start_Date
, a.Id
, a.Pending_Review_By
, a.Requested_By
, a.Requested_By_Full_Name
, a.Requested_DateTime
, a.[Status]
, a.Status_Description
, a.Total_Cost 
, c.Final_Reviewed_By

ORDER BY a.Requested_DateTime DESC</CommandText>
